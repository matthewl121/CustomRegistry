{
  "dependencies": {
    "@aws-sdk/client-s3": "^3.701.0",
    "adm-zip": "^0.5.10",
    "commander": "^11.1.0",
    "cors": "^2.8.5",
    "express": "^4.21.1",
    "fs": "^0.0.1-security",
    "http": "^0.0.1-security",
    "https": "^1.0.0",
    "tar": "^6.1.11"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.0.0",
    "jest": "^29.7.0",
    "jest-html-reporter": "^3.10.2",
    "jest-junit": "^16.0.0",
    "ts-jest": "^29.2.5",
    "typescript": "^5.0.0"
  },
  "scripts": {
    "start": "node server.js",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:rate": "node --experimental-vm-modules node_modules/jest/bin/jest.js lambda_test/ratePackage_test/index.test.mjs",
    "test:download": "node --experimental-vm-modules node_modules/jest/bin/jest.js lambda_test/download_test/index.test.mjs",
    "test:get": "node --experimental-vm-modules node_modules/jest/bin/jest.js lambda_test/getPackages_test/index.test.mjs",
    "test:update": "node --experimental-vm-modules node_modules/jest/bin/jest.js lambda_test/update_test/index.test.mjs",
    "test:cost": "node --experimental-vm-modules node_modules/jest/bin/jest.js lambda_test/costPackage_test/index.test.mjs",
    "test:tracks": "node --experimental-vm-modules node_modules/jest/bin/jest.js lambda_test/getTracks_test/index.test.mjs",
    "test:reset": "node --experimental-vm-modules node_modules/jest/bin/jest.js lambda_test/reset_test/index.test.mjs",
    "test:upload": "node --experimental-vm-modules node_modules/jest/bin/jest.js lambda_test/upload_test/index.test.mjs",
    "compile": "tsc",
    "install-deps": "npm install --save-dev && tsc --init"
  },
  "jest": {
    "transform": {},
    "testEnvironment": "node",
    "moduleFileExtensions": [
      "js",
      "mjs",
      "cjs",
      "json",
      "ts"
    ],
    "testMatch": [
      "**/*.test.mjs",
      "**/*.test.ts"
    ],
    "testTimeout": 30000,
    "moduleNameMapper": {
      "^(\\.{1,2}/.*)\\.js$": "$1"
    },
    "coverageReporters": [
      "text",
      "html"
    ],
    "reporters": [
      "default",
      [
        "jest-html-reporter",
        {
          "pageTitle": "Test Report",
          "outputPath": "./test-report.html"
        }
      ],
      [
        "jest-junit",
        {
          "outputDirectory": ".",
          "outputName": "junit.xml",
          "ancestorSeparator": " â€º ",
          "uniqueOutputName": "false",
          "suiteNameTemplate": "{filepath}",
          "classNameTemplate": "{classname}",
          "titleTemplate": "{title}"
        }
      ]
    ]
  },
  "type": "module"
}